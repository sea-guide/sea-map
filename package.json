  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-elm": "^2.8.2",
    "elm": "^0.19.1-3",
    "elm-format": "^0.8.4",
    "elm-hot": "^1.1.5",
    "elm-test": "^0.19.0-rev6",
    "netlify-plugin-cache": "^1.0.3",
    "node-elm-compiler": "^5.0.5",
    "parcel": "^2.8.2",
    "parcel-plugin-asset-copier": "^1.1.1"
  },
  "assetsPath": "src/images"
}
