{
  "name": "sea-map",
  "version": "0.0.0",
  "description": "SEA-MAP Tech vs. Abuse application",
  "scripts": {
    "start": "parcel src/index.html",
    "build": "npm test && parcel build src/index.html && npm run copyassets",
